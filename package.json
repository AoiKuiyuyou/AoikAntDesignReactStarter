{
  "": "# ------ 4G9K5 ------",
  "name": "AoikAntDesignReactStarter",
  "version": "0.0.1",
  "description": "Ant Design React starter kit",
  "scripts": {
    "dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "dev:mock": "cross-env REACT_APP_ENV=dev MOCK=true PORT=8000 umi dev",
    "tsc": "tsc --noEmit",
    "eslint": "eslint --config .eslintrc.js --no-color --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "eslint:fix": "eslint --config .eslintrc.js --no-color --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "stylelint": "stylelint --config .stylelintrc.js \"src/**/*.less\" --syntax less",
    "stylelint:fix": "stylelint --config .stylelintrc.js --fix \"src/**/*.less\" --syntax less",
    "build": "cross-env REACT_APP_ENV=prod umi build",
    "build:analyze": "cross-env REACT_APP_ENV=prod ANALYZE=1 umi build"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
